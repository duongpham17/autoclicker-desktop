{
  "name": "frontend",
  "productName": "Autoclicker",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@jitsi/robotjs": "^0.6.10",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.11.59",
    "@types/react": "^18.0.20",
    "@types/react-dom": "^18.0.6",
    "nft.storage": "^7.0.0",
    "node-gyp": "^9.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.4.0",
    "react-image-file-resizer": "^0.4.8",
    "react-router-dom": "^6.4.0",
    "react-scripts": "5.0.1",
    "robotjs": "^0.6.0",
    "sass": "^1.54.9",
    "typescript": "^4.8.3",
    "uuid": "^9.0.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/128x128.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/128x128.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Autoclicker\"",    
    "dev": "concurrently -k \"BROWSER=none npm start\" \"npm:electron\"",
    "electron": "wait-on tcp:3000 && electron .",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "rebuild": "npm rebuild --runtime=electron --target=17.1.0 --disturl=https://atom.io/download/atom-shell --abi=102"
  },
  "main": "public/electron.js",
  "homepage": "./",
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/uuid": "^8.3.4",
    "concurrently": "^7.4.0",
    "electron": "^20.1.3",
    "electron-is-dev": "^2.0.0",
    "electron-packager": "^16.0.0",
    "wait-on": "^6.0.1"
  }
}
